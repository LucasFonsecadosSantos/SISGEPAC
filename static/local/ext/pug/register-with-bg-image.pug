.app-content.content
  .content-header.row
  .content-overlay
  .content-wrapper
    .content-body
      section.row.flexbox-container
        .col-12.d-flex.align-items-center.justify-content-center
          .col-lg-4.col-md-8.col-10.box-shadow-2.p-0
            .card.border-grey.border-lighten-3.px-1.py-1.m-0
              .card-header.border-0.pb-0
                .card-title.text-center
                  img(src='../../../app-assets/images/logo/logo-dark.png' alt='branding logo')
                h6.card-subtitle.line-on-side.text-muted.text-center.font-small-3.pt-2
                  span Easily Using
              .card-content
                .text-center
                  a.btn.btn-social-icon.mr-1.mb-1.btn-outline-facebook(href='#')
                    span.la.la-facebook
                  a.btn.btn-social-icon.mr-1.mb-1.btn-outline-twitter(href='#')
                    span.la.la-twitter
                  a.btn.btn-social-icon.mr-1.mb-1.btn-outline-linkedin(href='#')
                    span.la.la-linkedin.font-medium-4
                  a.btn.btn-social-icon.mr-1.mb-1.btn-outline-github(href='#')
                    span.la.la-github.font-medium-4
                p.card-subtitle.line-on-side.text-muted.text-center.font-small-3.mx-2.my-1
                  span
                    | OR Using
                    | Email
                .card-body
                  form.form-horizontal(action='index.html' novalidate='')
                    fieldset.form-group.position-relative.has-icon-left
                      input#user-name.form-control(type='text' placeholder='User Name')
                      .form-control-position
                        i.la.la-user
                    fieldset.form-group.position-relative.has-icon-left
                      input#user-email.form-control(type='email' placeholder='Your Email Address' required='')
                      .form-control-position
                        i.la.la-envelope
                    fieldset.form-group.position-relative.has-icon-left
                      input#user-password.form-control(type='password' placeholder='Enter Password' required='')
                      .form-control-position
                        i.la.la-key
                    .form-group.row
                      .col-sm-6.col-12.text-center.text-sm-left.pr-0
                        fieldset
                          input#remember-me.chk-remember(type='checkbox')
                          label(for='remember-me')  Remember Me
                      .col-sm-6.col-12.float-sm-left.text-center.text-sm-right
                        a.card-link(href='recover-password.html') Forgot Password?
                    button.btn.btn-outline-info.btn-block(type='submit')
                      i.la.la-user
                      |  Register
                .card-body
                  a.btn.btn-outline-danger.btn-block(href='login-with-bg-image.html')
                    i.ft-unlock
                    |               Login
